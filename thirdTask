import java.util.Scanner;

public class thirdHomework {
    public static void main(String[] args) {
        goodbye();

    }
    public static void goodbye() {
        Scanner scan = new Scanner(System.in);
        int floor = scan.nextInt();
        for (int i = 0; i < floor; i++){
            switch(i) {
                case 1:
                    System.out.println("Сбогом Сашке, 8 етаж");
                    break;
                case 2:
                    System.out.println("Сбогом Петя, 7 етаж");
                    break;
                case 3:
                    System.out.println("Сбогом Марийка, 6 етаж");
                    break;
                case 4:
                    System.out.println("Сбогом Иванка, 5 етаж");
                    break;
                case 5:
                    System.out.println("Сбогом Станка, 4 етаж");
                    break;
                case 6:
                    System.out.println("Сбогом Поли, 3 етаж");
                    break;
                case 7:
                    System.out.println("Сбогом Сара, 2 етаж");
                    break;
                case 8:
                    System.out.println("Аучч");
                    break;

            }

        }
    }
}
